{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@types": ["types/index.ts"],
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
