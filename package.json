{
  "name": "personal-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev --host",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview --host",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/react": "3.0.9",
    "@astrojs/tailwind": "5.1.0",
    "@headlessui/react": "^1.7.13",
    "@headlessui/tailwindcss": "^0.1.2",
    "astro": "4.2.1",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.4",
    "tailwindcss": "^3.3.1",
    "tw-merge": "^0.0.1-alpha.3"
  },
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.13",
    "prettier-plugin-tailwindcss": "^0.5",
    "typescript": "^5.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
